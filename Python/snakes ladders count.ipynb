    "n=input()\n",
    "n=n.split(',')\n",
    "ladders=0\n",
    "snakes=0\n",
    "for i in n:\n",
    "    a=i.split(':')\n",
    "    num1=int(a[0])\n",
    "    num2=int(a[1])\n",
    "    if num2-num1>0:\n",
    "        ladders+=1\n",
    "    else:\n",
    "        snakes+=1\n",
    "print(snakes,' ',ladders,end='')\n",
    "    "
