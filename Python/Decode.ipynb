    "import string\n",
    "n=input()\n",
    "n=n.upper()\n",
    "letters=string.ascii_letters\n",
    "letters=letters.upper()\n",
    "letters=letters[:26]\n",
    "decode=\"\"\n",
    "for i in range(0,len(n)):\n",
    "    pos=letters.index(n[i])\n",
    "    decode=letters[(pos-3)%26]+decode\n",
    "print(decode)"
